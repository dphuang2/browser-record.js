{
    "name": "Injecta",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Injecting stuff",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "inject.js"
            ],
            "all_frames": true
        }
    ],
    "web_accessible_resources": [
        "br.js"
    ]
}